(def turn-left  {:up :left :down :right :left :down :right :up})
(def turn-right {:up :right :down :left :left :up :right :down})

(let [lines (->> i (str/split-lines))
      x     (->> lines first (map-indexed vector) (remove #(= \space (second %))) (ffirst))
      y     0]
  (loop [word ""
         x    x
         y    y
         dir  :down
         n    1]
    ;(prn dir x y n word)
    (let [ch       (get-in lines [y x])
          word     (cond-> word
                     (some-> ch int Character/isAlphabetic) (str ch))
          straight (fn [dir]
                     (let [[x y] (u/next-xy dir [x y])
                           ch    (get-in lines [y x])]
                       (when (some->> ch (not= \space))
                         [dir x y])))
          turn     (fn [dir]
                     (or (straight (turn-left dir))
                         (straight (turn-right dir))))]
      (if-let [[dir x y] (or (straight dir) (turn dir))]
        (recur word x y dir (inc n))
        (prn [word n])))))

