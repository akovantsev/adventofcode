(defn gen-floats [mask]
  (->> (range 36)
    (remove mask)
    (reduce
      (fn [masks idx]
        (into
          (map #(assoc % idx 1) masks)
          (map #(assoc % idx 0) masks)))
      [{}])))

(gen-floats (mask "X00000000000000000000000000000X1001X"))

(defn apply-mask [idx mask]
  (reduce-kv
    (fn [idx i v]
      (case v
        \0 idx
        \1 (assoc idx i 1)))
    idx
    mask))


(defn step2 [state line]
  (if (str/starts-with? line "mask")
    (let [m (mask line)]
      (assoc state :mask m :floats (gen-floats m)))
    (let [[idx x] (re-seq #"\d+" line)
          v    (read-string x)
          idx  (-> idx tobinmap (apply-mask (:mask state)))
          idxs (->> state :floats (map #(merge idx %)) (map toint))]
      (reduce #(assoc %1 %2 v) state idxs))))

(defn calc2 [state]
  (-> state
    (dissoc :mask :floats)
    (vals)
    (->> (reduce +))))

