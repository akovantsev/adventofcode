(def zero (vec (repeat 36 \0)))

(defn toint [m]
  (Long/parseLong
    (->> m (reduce (partial apply assoc) zero) reverse (str/join))
    2))

(defn mask [s]
  (->> (str/split s #"\s")
    peek
    reverse
    (map-indexed vector)
    (remove #(-> % second #{\X}))
    (into {})))

(defn calc [state]
  (-> state
    (dissoc :mask)
    (vals)
    (->>
      (map toint)
      (reduce +))))

(defn tobinmap [s]
  (->> s read-string Integer/toBinaryString reverse (map-indexed vector) (into {})))


(defn step [state line]
  (if (str/starts-with? line "mask")
    (assoc state :mask (mask line))
    (let [[idx x] (re-seq #"\d+" line)
          x   (-> x tobinmap (merge (:mask state)))]
      (assoc state idx x))))


(prn (->> i str/split-lines (reduce step {}) calc))
